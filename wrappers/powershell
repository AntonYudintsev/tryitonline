#!/usr/bin/env bash

export HOME=/tmp/home LD_LIBRARY_PATH=/opt/microsoft/lib64
ln -s /home/runner $HOME
ln .code.tio .code.tio.ps1
/opt/microsoft/dotnet/dotnet /opt/microsoft/powershell/6.0.0-alpha.15/powershell.ni.exe -Command 'cd; /opt/microsoft/lib64/aliases.ps1; Get-Content .input.tio | ./.code.tio.ps1' "$@"
